    <!-- http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Закрытые_корпоративные_порталы -->

    <owl:Class rdf:about="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Закрытые_корпоративные_порталы">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Портал"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Имеет_вид"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Имеет_технологию"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Технологии"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Имеет_доступ"/>
                        <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Закрытый"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Имеет_тип"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Корпоративный"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Имеет_целевую_задачу"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Целевая_задача"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kotwm/ontologies/2018/4/portals#Портал"/>
    </owl:Class>
    


